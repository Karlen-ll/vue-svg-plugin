{
  "name": "vue-svg-plugin",
  "version": "1.0.0",
  "description": "Load SVG files as Vue components",
  "keywords": [
    "component",
    "plugin",
    "loader",
    "webpack",
    "rspack",
    "vite",
    "vue",
    "svg"
  ],
  "type": "module",
  "main": "./dist/core.cjs",
  "module": "./dist/core.mjs",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.mjs",
      "require": "./dist/vite-plugin.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack-plugin.d.ts",
      "import": "./dist/webpack-plugin.mjs",
      "require": "./dist/webpack-plugin.cjs"
    },
    "./webpack-loader": {
      "types": "./dist/webpack-loader.d.ts",
      "import": "./dist/webpack-loader.mjs",
      "require": "./dist/webpack-loader.cjs"
    },
    "./types": {
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:pack": "npm run build && npm pack",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-istanbul": "^4.0.17",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "6.3.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.15",
    "webpack": "^5.104.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karlen-ll/vue-svg-plugin.git"
  },
  "author": {
    "name": "Karlen Pireverdiev",
    "email": "ll.karlen@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karlen-ll/vue-svg-plugin/issues"
  },
  "homepage": "https://github.com/Karlen-ll/vue-svg-plugin#readme"
}
